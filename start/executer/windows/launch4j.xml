<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>
  <headerType>0</headerType>
  <jar>gtitool.start-@gtitool.version@.jar</jar>
  <outfile>../../dist/gtitool.exe</outfile>
  <errTitle>GTI Tool: Error</errTitle>
  <customProcName>true</customProcName>
  <icon>gtitool.ico</icon>
  <jre>
    <minVersion>1.1.0</minVersion>
  </jre>
  <versionInfo>
    <fileVersion>@gtitool.version@.0</fileVersion>
    <txtFileVersion>@gtitool.version@.0</txtFileVersion>
    <fileDescription>GTI Tool</fileDescription>
    <copyright>Copyright (c) 2007-2008 C. Fehler, B. Mies</copyright>
    <productVersion>@gtitool.version@.0</productVersion>
    <txtProductVersion>@gtitool.version@.0</txtProductVersion>
    <productName>GTI Tool</productName>
    <companyName>University of Siegen, Department of Theoretical Computer Science</companyName>
    <internalName>GTI Tool</internalName>
    <originalFilename>gtitool.exe</originalFilename>
  </versionInfo>
</launch4jConfig>